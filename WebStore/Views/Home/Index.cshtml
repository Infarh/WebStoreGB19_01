@model IEnumerable<WebStore.Models.EmployeeViewModel>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <title>WebStore app</title>
</head>
<body>
    <section>
        <div>
            <h3>Сотрудники</h3>
            <table>
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Фамилия</th>
                        <th>Имя</th>
                        <th>Отчество</th>
                        <th>Возраст</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var employee in Model)
                    {
                        <tr>
                            <td>@employee.Id</td>
                            <td>@employee.SecondName</td>
                            <td>@employee.FirstName</td>
                            <td>@employee.Patronymic</td>
                            <td>@employee.Age</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </section>
</body>
</html>